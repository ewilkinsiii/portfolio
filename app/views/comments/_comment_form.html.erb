 <% unless current_user.is_a? GuestUser %>
  <%= form_for @comment, url: '#' do |f| %>
   <div class='form-group'>
    <%= fa_icon "comment"%> <%= f.label "Post a Comment" %>
    <%= f.text_area :content, class: 'form-control' %>
   </div>
   
   <%= f.submit 'Post Comment', class: 'btm btn-primary' %>
  <%end%>
 <%end%>